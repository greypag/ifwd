<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d [%thread] %-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	<root level="warn">
		<appender-ref ref="STDOUT" />	
	</root>
	
	<logger name="com.ifwd" level="debug" />
	
</configuration>